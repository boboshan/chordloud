{
	"name": "qord",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"build:types": "svelte-kit sync && tsc --emitDeclarationOnly",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"type-check": "tsc --noEmit",
		"type-check:watch": "tsc --noEmit --watch",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint . --fix",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"test:types": "npm run type-check",
		"ci": "npm run lint && npm run type-check && npm run test",
		"clean": "rimraf dist .svelte-kit"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.38.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^24",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.14",
		"rimraf": "^6.0.1",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.1",
		"vite": "^7.1.10",
		"vitest": "^4.0.5"
	},
	"description": "A comprehensive TypeScript music theory library for modern web applications. Provides classes and utilities for pitch classes, notes, intervals, chords, and pitch class sets with full type safety.",
	"keywords": [
		"music",
		"music theory",
		"pitch class",
		"note",
		"chord",
		"interval",
		"scale",
		"key",
		"harmony",
		"set theory",
		"music analysis",
		"typescript",
		"svelte"
	],
	"author": "bbs"
}
